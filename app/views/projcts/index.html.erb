<h1>Listing projcts</h1>

<table>
  <thead>
    <tr>
      <th>Index</th>
      <th>Create</th>
      <th>Edit</th>
      <th>Show</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @projcts.each do |projct| %>
      <tr>
        <td><%= projct.index %></td>
        <td><%= projct.create %></td>
        <td><%= projct.edit %></td>
        <td><%= projct.show %></td>
        <td><%= link_to 'Show', projct %></td>
        <td><%= link_to 'Edit', edit_projct_path(projct) %></td>
        <td><%= link_to 'Destroy', projct, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Projct', new_projct_path %>
